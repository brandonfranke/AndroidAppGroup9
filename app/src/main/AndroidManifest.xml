<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.votingapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Admin.addCandidate" />
        <activity android:name=".Admin.CreateElectionActivity" />
        <activity android:name=".Users.ElectionDetailActivity" />
        <activity android:name=".Admin.createCandProfile" />
        <activity android:name=".Admin.AdminHomeActivity" />
        <activity android:name=".RegisterLogin.SetupActivity" />
        <activity android:name=".Users.UserHomeActivity" />
        <activity android:name=".Admin.electionView" />
        <activity android:name=".Admin.electionDescription"/>
        <activity android:name=".Admin.SetFreezeTime"/>
        <activity android:name=".Admin.SetFreezeTimePresentElection"/>
        <activity android:name=".RegisterLogin.LoginActivity" />
        <activity android:name=".RegisterLogin.RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>